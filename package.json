{
  "name": "animationis",
  "version": "0.8.3",
  "description": "A scripting APNG generator",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build-watch": "babel src --out-dir lib --watch",
    "test": "mocha --require @babel/register --recursive test/unit",
    "test-watch": "mocha --require @babel/register --watch --recursive test/unit"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/suitougreentea/animationis.git"
  },
  "author": "suitougreentea",
  "license": "MIT",
  "dependencies": {
    "command-exists": "^1.2.2",
    "commander": "^2.14.1",
    "interpret": "^1.1.0",
    "loglevel": "^1.6.1",
    "rechoir": "^0.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-plugin-espower": "^3.0.0-beta.1",
    "canvas": "^2.0.0-alpha.9",
    "canvas-prebuilt": "^2.0.0-alpha.5",
    "mocha": "^5.0.1",
    "power-assert": "^1.4.4"
  },
  "bin": {
    "animationis": "bin/animationis"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "env": {
      "development": {
        "plugins": [
          "babel-plugin-espower"
        ]
      }
    }
  }
}
